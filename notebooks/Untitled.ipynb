   "source": [
    "%matplotlib inline \n",
    "\n",
    "import pandas as pd\n",
    "import numpy as np\n",
    "import matplotlib.pyplot as plt\n",
    "\n",
    "sin = np.sin\n",
    "cos = np.cos\n",
    "pi = np.pi\n",
    "N = 100\n",
    "\n",
    "x = np.linspace(0, pi, N)\n",
    "a = sin(x)\n",
    "b = cos(x)\n",
    "\n",
    "df = pd.DataFrame({\n",
    "    'A': [True]*N + [False]*N,\n",
    "    'B': np.hstack((a,b))\n",
    "    })\n",
    "df\n"
   ]
